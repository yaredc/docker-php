    "bootswatch": "^4.3.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "node-sass": "^4.12.0",
    "popper.js": "^1.15.0",
    "typescript": "^3.6.2"
  },
  "engines": {
    "node": "^12"
  }
